{
  "builds": [
    { "src": "public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/demo", "dest": "/demo.html" },
    { "src": "^/UMBRELLAWEBSITE-main/dist/(.*)$", "dest": "/UMBRELLAWEBSITE-main/dist/$1" },
    { "src": "^/UMBRELLAWEBSITE-main/(.*\\.(js|css|map|png|jpg|jpeg|svg|json))$", "dest": "/UMBRELLAWEBSITE-main/dist/$1" },
    { "src": "^/UMBRELLAWEBSITE-main/?$", "dest": "/UMBRELLAWEBSITE-main/dist/index.html" },
    { "src": "^/UMBRELLAWEBSITE-main/(.*)$", "dest": "/UMBRELLAWEBSITE-main/dist/index.html" }
  ]
}
